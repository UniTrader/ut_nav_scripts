<?xml version="1.0" encoding="UTF-8"?>
<replace sel="/aiscript/attention/actions/do_if[run_script[@name='&apos;move.generic&apos;']]">
      <do_if value="(this.zone != $gate.zone) or this.ship.isclass.ship_xl or this.ship.isclass.ship_l">
        <do_if value="this.ship.isclass.ship_xl or this.ship.isclass.ship_l">
          <set_value name="$bigship"/>
        </do_if>
        <run_script name="if $bigship then 'ut.cac.move.gate.wrapper' else 'move.generic'" >
          <param name="destination" value="$gate.zone"/>
          <param name="gate" value="$gate"/>
          <param name="endintargetzone" value="true"/>
          <param name="debugchance" value="$debugchance"/>
        </run_script>
        <do_if value="$bigship">
          <return/>
        </do_if>
      </do_if>
</replace>
